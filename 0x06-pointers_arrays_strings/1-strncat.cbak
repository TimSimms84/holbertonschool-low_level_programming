#include "holberton.h"

/**
 * _strncat - appends n src to dest string
 * @dest: string that gets appended
 * @src: strig to append
 * @n: number in string to append
 *
 * Return: dest
 */

char *_strncat(char *dest, char *src, int n)
{
	int i = 0, j = 0;

	while (dest[i])
		i++;
	while (j < n && src[j])
	{
		i++;
		j++;
	}
	if (j < n)
		dest[i] = src[j];


return (dest);
}
